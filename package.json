{
  "name": "heimdall-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test:js": "mocha 'test/**/*.test.mjs'",
    "test:elm": "elm-test 'tests/**/*Test.elm'",
    "test:e2e": "cypress run",
    "e2e": "npm run test:e2e",
    "test": "npm run test:elm && npm run test:js",
    "start": "elm-live src/Main.elm --port=8001 --dir=./src --start-page=index.html --pushstate -- --debug --output=src/elm.js",
    "serve:dist": "serve -l 8001 -s dist",
    "format": "elm-format src/Main.elm",
    "review": "elm-review",
    "graphql:generate": "elm-graphql --schema-file src/eventsSchema.graphql --base Api",
    "generate:graphql": "npm run graphql:generate",
    "generate:jwt": "node generateJWT.js",
    "compile:elm": "elm make src/Main.elm --output=src/elm.js",
    "uglify:elm": "npx uglifyjs src/elm.js --compress 'pure_funcs=[F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9],pure_getters,keep_fargs=false,unsafe_comps,unsafe' | npx uglifyjs --mangle --output src/elm.js",
    "compile:css": "tailwindcss -c tailwind.config.cjs -i ./src/tailwind.css -o src/tailwind.build.css",
    "compile:css:prod": "NODE_ENV=production npm run compile:css",
    "compile": "npm run compile:elm && npm run compile:css:prod",
    "watch:css": "tailwindcss -c tailwind.config.cjs -i ./src/tailwind.css -o src/tailwind.build.css --watch",
    "build": "npm run clean && npm run compile && npm run copy",
    "clean": "rm -rf public",
    "copy": "mkdir public && cp src/elm.js public/elm.js && cp src/elmApp.js public/elmApp.js && cp src/environment.js public/environment.js && cp src/token.js public/token.js && cp src/index.html public/index.html && cp src/tailwind.build.css public/tailwind.build.css",
    "deploy": "sls deploy",
    "canary:zip": "rm synthetic-canary.zip | true && cd ./synthetic-canary && zip -r synthetic-canary.zip ./nodejs/* && cd - && mv ./synthetic-canary/synthetic-canary.zip ./synthetic-canary.zip"
  },
  "repository": {
    "type": "git",
    "url": "stuff.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "stuff/issues"
  },
  "homepage": "stuff#readme",
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^4.2.2",
    "autoprefixer": "^10.4.0",
    "aws-sdk": "^2.1066.0",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "cypress": "^10.6.0",
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5",
    "elm-live": "^4.0.2",
    "elm-review": "^2.6.1",
    "elm-test": "^0.19.1-revision7",
    "esm": "^3.2.25",
    "ini": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "kill": "^0.0.2",
    "kill-port": "^1.6.1",
    "mocha": "^9.1.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "pm2": "^5.2.0",
    "postcss": "^8.3.11",
    "serve": "^13.0.2",
    "serverless": "^3.15.2",
    "serverless-appsync-plugin": "^1.12.1",
    "tailwindcss": "^3.0.17",
    "uglify-js": "^3.15.2",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
  }
}
